{
  "name": "svelte-check-rs-repro",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:rs": "svelte-check-rs --threshold error"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/kit": "^2.49.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.3",
    "svelte": "^5.46.1",
    "svelte-check": "^4.3.5",
    "svelte-check-rs": "0.5.5",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "pnpm": {
    "allowBuilds": [
      "esbuild",
      "svelte-check-rs"
    ]
  }
}
